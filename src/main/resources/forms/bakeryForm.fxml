<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="mainPane" prefHeight="476.0" prefWidth="825.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.graphic.ui.controllers.BakeryForm">
    <HBox>
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <VBox prefHeight="561.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <Label text="В ожидании"/>
                    <ListView fx:id="inWaitingListView" layoutY="14.0" prefHeight="173.0" prefWidth="200.0"
                              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="14.0"/>
                </AnchorPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <Label text="Готовятся"/>
                    <ListView fx:id="cookingListView" layoutY="14.0" prefHeight="173.0" prefWidth="200.0"
                              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="14.0"/>
                </AnchorPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <Label text="Готовы"/>
                    <ListView fx:id="readyListView" layoutY="14.0" prefHeight="173.0" prefWidth="200.0"
                              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="14.0"/>
                </AnchorPane>
            </VBox>
        </AnchorPane>
        <AnchorPane>
            <ImageView id="emtyImage" fx:id="emptyImage" fitHeight="402.0" fitWidth="622.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../img/bakery/empty.png"/>
            </ImageView>
            <ImageView id="emtyImage" fx:id="girlImage" fitHeight="402.0" fitWidth="622.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../img/bakery/girl.png"/>
            </ImageView>
            <ImageView id="emtyImage" fx:id="boyImage" fitHeight="402.0" fitWidth="622.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../img/bakery/boy.png"/>
            </ImageView>
            <ImageView id="emtyImage" fx:id="unicornImage" fitHeight="561.0" fitWidth="909.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../img/bakery/unicorn.png"/>
            </ImageView>
            <ImageView fx:id="cakeImage" fitHeight="79.0" fitWidth="79.0" layoutX="270.0" layoutY="308.0"
                       pickOnBounds="true" preserveRatio="true">
                <Image url="@../img/bakery/Chocolate_Cake.png"/>
            </ImageView>
            <Button fx:id="cakeButton" layoutX="302.0" layoutY="330.0" mnemonicParsing="false"/>
            <ImageView id="cakeImage" fx:id="boxImage" fitHeight="72.21500015258789" fitWidth="123.44444429770239"
                       layoutX="248.0" layoutY="311.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../img/bakery/White_Gift_Box_Png_Clipart_Image.png"/>
            </ImageView>
            <ImageView id="cakeImage" fx:id="rainbowImage" fitHeight="62.21500778198242"
                       fitWidth="114.46484070270887" layoutX="252.0" layoutY="311.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../img/bakery/rainbow.png"/>
            </ImageView>
            <ImageView id="cakeImage" fx:id="magicImage" fitHeight="99.81122563262129" fitWidth="101.44449615478516"
                       layoutX="259.0" layoutY="293.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../img/bakery/magic.png"/>
            </ImageView>
            <ImageView fx:id="customerDialogImage" fitHeight="178.73479102602536" fitWidth="261.5631103515625"
                       layoutX="476.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../img/bakery/dialog-bubble-png-5.png"/>
            </ImageView>
            <TextArea fx:id="customerTextArea" editable="false" layoutX="486.0" layoutY="34.0" prefHeight="117.0"
                      prefWidth="241.0" wrapText="true"/>
            <ImageView fx:id="sellerDialogImage" fitHeight="173.41836722539594" fitWidth="199.94117736816406"
                       layoutX="15.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../img/bakery/img_454966.png"/>
            </ImageView>
            <TextArea fx:id="sellerTextArea" editable="false" layoutX="28.0" layoutY="48.0" prefHeight="117.0"
                      prefWidth="175.0" wrapText="true"/>
            <Button fx:id="nextStepButton" layoutX="13.0" layoutY="522.0" mnemonicParsing="false"
                    onMouseClicked="#nextStep" text="-&gt;"/>
            <AnchorPane fx:id="boxPane" layoutX="507.0" layoutY="281.0" prefHeight="200.0" prefWidth="200.0"
                        style="-fx-background-color: white;">
                <RadioButton fx:id="redColor" mnemonicParsing="false" text="Красный"/>
                <RadioButton fx:id="greenColor" layoutY="28.0" mnemonicParsing="false" text="Зеленый"/>
                <RadioButton fx:id="blueColor" layoutX="1.0" layoutY="59.0" mnemonicParsing="false"
                             text="Голубой"/>
                <CheckBox fx:id="withBow" layoutX="1.0" layoutY="92.0" mnemonicParsing="false"
                          text="С бантиком"/>
            </AnchorPane>
        </AnchorPane>
    </HBox>
</AnchorPane>
